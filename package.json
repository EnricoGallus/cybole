{
  "name": "cybole",
  "version": "0.1.3",
  "private": true,
  "homepage": ".",
  "main": "build/electron.js",
  "author": "Enrico Gallus <enrico.gallus@gmail.com>",
  "description": "Editor for CYBOL-Projects",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --watchAll=false --coverage",
    "postinstall": "electron-builder install-app-deps",
    "electron:dev": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && tsc -p electron -w\" \"wait-on http://localhost:3000 && tsc -p electron && electron .\"",
    "electron:build": "yarn build && tsc -p electron && electron-builder",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@geist-ui/core": "^2.3.8",
    "@geist-ui/icons": "^1.0.2",
    "@sentry/electron": "^4.0.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.2.1",
    "fast-xml-parser": "^4.0.9",
    "react": "^18.2.0",
    "react-base-table": "^1.13.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "react-overlays": "^5.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-xml-editor": "^0.10.0",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.7",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.34.0",
    "@typescript-eslint/parser": "^5.34.0",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "electron": "^20.1.0",
    "electron-builder": "^23.3.3",
    "electronmon": "^2.0.2",
    "eslint": "^8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.8.7",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.8",
    "typescript": "^4.7.4",
    "wait-on": "^6.0.1"
  },
  "build": {
    "appId": "org.electron.cybole",
    "productName": "cybole",
    "files": [
      "build/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb",
      "category": "Development",
      "maintainer": "Enrico Gallus",
      "synopsis": "cybole"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:EnricoGallus/cybole.git"
  },
  "bugs": {
    "url": "https://github.com/EnricoGallus/cybole/issues"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
