{
  "name": "cybole",
  "version": "0.1.3",
  "private": true,
  "homepage": "https://github.com/EnricoGallus/cybole#readme",
  "main": "./dist/main.bundle.js",
  "author": "Enrico Gallus <enrico.gallus@gmail.com>",
  "description": "Editor for CYBOL-Projects",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --color",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack --progress --color",
    "test": "cross-env NODE_ENV=test jest",
    "watch-test": "npm run test -- --watchAll",
    "next-rc": "npm --no-git-tag-version version prerelease --preid=rc",
    "next-patch": "npm --no-git-tag-version version patch",
    "next-minor": "npm --no-git-tag-version version minor",
    "next-major": "npm --no-git-tag-version version major",
    "build:win": "electron-builder build --win",
    "build:mac": "electron-builder build --mac"
  },
  "dependencies": {
    "@geist-ui/core": "^2.3.8",
    "@geist-ui/icons": "^1.0.2",
    "@sentry/electron": "^3.0.7",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.0.5",
    "fast-xml-parser": "^4.0.9",
    "react": "^18.2.0",
    "react-base-table": "^1.13.2",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.33.1",
    "react-overlays": "^5.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "react-xml-editor": "^0.10.0",
    "typescript": "^4.7.4",
    "uuid": "^8.3.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "cross-env": "^7.0.3",
    "electron": "^19.0.9",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.6.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "electron-builder": "^23.1.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.3.1",
    "tsconfig-paths": "^4.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "build": {
    "appId": "org.electron.cybole",
    "productName": "CYBOLE",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "dmg",
      "arch": "universal"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb",
      "category": "Development",
      "maintainer": "Enrico Gallus",
      "synopsis": "CYBOLE"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:EnricoGallus/cybole.git"
  },
  "bugs": {
    "url": "https://github.com/EnricoGallus/cybole/issues"
  }
}
